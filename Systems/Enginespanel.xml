				<tableData>
					-1.0		0.0
					0.0		0.0
					999.0		999.0
					1000.0		999.0
				</tableData>
			</table>
		</function>
		</fcs_function>

		<fcs_function name="systems/enginespanel/apoapsis-hundreds-km">
		<function>
			<integer>	
				<quotient>
					<property>systems/enginespanel/apoapsis-indicated-km</property>
