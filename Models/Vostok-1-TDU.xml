	<enable-hot type="bool">false</enable-hot>

	<animation>
		<type>select</type>
		<condition>
			<and>
				<or>
					<equals>
						<property>fdm/jsbsim/stages/unit[0]/active</property>
						<value>true</value>
					</equals>
					<equals>
						<property>fdm/jsbsim/stages/unit[2]/active</property>
						<value>true</value>
					</equals>
					<equals>
						<property>fdm/jsbsim/stages/unit[3]/active</property>
						<value>true</value>
					</equals>
					<equals>
						<property>fdm/jsbsim/stages/unit[4]/active</property>
						<value>true</value>
					</equals>
				</or>
				<and>
					<equals>
						<property>fdm/jsbsim/stages/unit[1]/active</property>
						<value>false</value>
					</equals>
					<equals>
						<property>sim/current-view/internal</property>
						<value>false</value>
					</equals>
				</and>
			</and>
		</condition>
	</animation>

	<animation>
		<type>translate</type>
		<property>fdm/jsbsim/stages/unit[4]/show-translate</property>
		<factor>1.0</factor>
