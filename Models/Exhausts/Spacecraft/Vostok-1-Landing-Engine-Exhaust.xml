<PropertyList>

	<path>Vostok-1-Landing-Engine-Exhaust.ac</path>

	<offsets>
		<x-m>0.0</x-m>
		<y-m>0.0</y-m>
		<z-m>0.0</z-m>
		<pitch-deg>0</pitch-deg>
	</offsets>

	<enable-hot type="bool">false</enable-hot>

	<animation>
		<type>material</type>
		<object-name>LandingEngineExhaust</object-name>
		<diffuse>
			<red-prop>fdm/jsbsim/propulsion/engine[40]/red</red-prop>
			<green-prop>fdm/jsbsim/propulsion/engine[40]/green</green-prop>
			<blue-prop>fdm/jsbsim/propulsion/engine[40]/blue</blue-prop>
		</diffuse>
		<ambient>
			<red-prop>fdm/jsbsim/propulsion/engine[40]/red</red-prop>
			<green-prop>fdm/jsbsim/propulsion/engine[40]/green</green-prop>
			<blue-prop>fdm/jsbsim/propulsion/engine[40]/blue</blue-prop>
		</ambient>
		<specular>
			<red-prop>fdm/jsbsim/propulsion/engine[40]/red</red-prop>
			<green-prop>fdm/jsbsim/propulsion/engine[40]/green</green-prop>
			<blue-prop>fdm/jsbsim/propulsion/engine[40]/blue</blue-prop>
		</specular>
		<emission>
			<red-prop>fdm/jsbsim/propulsion/engine[40]/red</red-prop>
			<green-prop>fdm/jsbsim/propulsion/engine[40]/green</green-prop>
			<blue-prop>fdm/jsbsim/propulsion/engine[40]/blue</blue-prop>
		</emission>
		<transparency>
			<alpha-prop>fdm/jsbsim/propulsion/engine[40]/transparency</alpha-prop>
		</transparency>
	</animation>


</PropertyList>
