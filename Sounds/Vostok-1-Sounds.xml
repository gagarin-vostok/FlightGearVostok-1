<PropertyList>

<!-- Sounds -->

	<fx>

	<!-- First and second stages  -->

	<wind>
		<name>wind</name>
		<mode>looped</mode>
		<path>Aircraft/Vostok-1/Sounds/Vostok-1-Wind.wav</path>
		<volume>
			<property>fdm/jsbsim/systems/sounds/wind</property>
			<factor>1.0</factor>
			<offset>0.0</offset>
			<min>0.0</min>
			<max>0.5</max>
		</volume>
		<pitch>
			<property>fdm/jsbsim/systems/sounds/wind</property>
			<factor>0.01</factor>
			<offset>1.0</offset>
		</pitch>
		<position>
			<x>0</x>
			<y>0</y>
			<z>0</z>
		</position>
		<max-dist>2</max-dist>
		<reference-dist>1</reference-dist>
	</wind>

